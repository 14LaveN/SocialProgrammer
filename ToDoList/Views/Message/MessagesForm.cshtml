@using SocialProgrammer.Domain.Entity;
@model List<MessageHistoryEntity>?

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>SocialProgrammer</title>
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@500;700&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="~/css/StandartStyle.css">
</head>
<body>
	@foreach(var item in Model)
	{
		<form>
			<h1>Reciepent: @item.ReciepentId</h1>
			<h1>Author: @item.Author</h1>
			<a asp-controller="Message" asp-action="MessageForm" asp-route-recipientName="@item.ReciepentId">Enter</a>
		</form>
	}
</body>
</html>