@using SocialProgrammer.Domain.Entity
@model List<ProfileEntity>

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>SocialProgrammer</title>
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@500;700&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="~/css/StandartStyle.css">
</head>
<body>
    <div class="container" id="container">
        @foreach (var item in Model)
        {
            <h2>@item.Name</h2>
            <h2>@item.Description</h2>
            <h2>@item.CreationDate</h2>
            <h2>@item.ArticlesCount</h2>
            <form asp-controller="Profile" asp-action="SearchedProfileForm" asp-route-id="@item.Id">
                <button>In</button>
            </form>
        }
    </div>
</body>
</html>